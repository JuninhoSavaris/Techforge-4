class Produto {
    nome: string;
    preco: number;
    quantidade: number;

    constructor(nome: string, preco: number, quantidade: number) {
        this.nome = nome;
        this.preco = preco;
        this.quantidade = quantidade;
    }

    valorTotalEmEstoque(): number {
        return this.preco * this.quantidade;
    }
}
const produto = new Produto("Notebook", 3500, 4);

console.log(`Valor total em estoque: R$${produto.valorTotalEmEstoque()}`);
