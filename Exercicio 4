class Temperatura {
  valor: number; // valor em Celsius

  constructor(valor: number) {
    this.valor = valor;
  }

  paraFahrenheit(): number {
    return (this.valor * 9) / 5 + 32;
  }

  paraKelvin(): number {
    return this.valor + 273.15;
  }
}

// Exemplo de uso
const t = new Temperatura(25);
console.log("Fahrenheit:", t.paraFahrenheit()); // 77
console.log("Kelvin:", t.paraKelvin()); // 298.15
